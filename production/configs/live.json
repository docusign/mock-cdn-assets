{
  "libraries": {
    "basePrefix": "https://1fe-a.akamaihd.net/integration/libs/"
  },
  "widgets": {
    "basePrefix": "https://1fe-a.akamaihd.net/integration/widgets/",
    "configs": [
      {
        "widgetId": "@1fe/bathtub",
        "type": "system",
        "plugin": {
          "enabled": true,
          "route": "/bathtub"
        }
      },
      {
        "widgetId": "@1fe/widget-starter-kit",
        "plugin": {
          "enabled": true,
          "route": "/widget-starter-kit"
        }
      },
      {
        "widgetId": "@1fe/sample-widget",
        "plugin": {
          "enabled": true,
          "route": "/sample-widget"
        }
      },
      {
        "widgetId": "@1fe/sample-widget-with-auth",
        "plugin": {
          "enabled": true,
          "route": "/sample-widget-with-auth",
          "auth": {
            "authenticationType": "required"
          }
        }
      }
    ]
  },
  "platform": {
    "devtools": {
      "importMapOverrides": {
        "enableUI": true,
        "allowedSources": ["localhost", "127.0.0.1", "1fe-a.akamaihd.net"]
      }
    },
    "browserslistConfig": {
      "buildTarget": [
        "defaults",
        "not dead",
        "edge>=19",
        "unreleased versions"
      ],
      "unsupportedBrowserScreen": [
        "defaults",
        "not dead",
        "edge>=19",
        "unreleased versions"
      ]
    }
  }
}
